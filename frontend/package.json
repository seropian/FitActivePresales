{
  "name": "fitactive-frontend",
  "private": true,
  "version": "0.0.1",
  "description": "FitActive presales frontend - React application with Tailwind CSS",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode production",
    "preview": "vite preview --host",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "echo \"Linting not configured\" && exit 0",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react",
    "vite",
    "tailwind",
    "fitness",
    "presales"
  ],
  "author": "FitActive Team",
  "license": "ISC",
  "dependencies": {
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.446.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.41",
    "postcss-load-config": "^6.0.1",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
