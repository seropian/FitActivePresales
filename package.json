{
  "name": "fitactive-presales-starter",
  "version": "0.0.1",
  "description": "FitActive presales application with payment processing",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "dev:server": "cd server && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd server && npm install && cd ../frontend && npm install",
    "clean": "cd frontend && npm run clean",
    "test:payment": "npx tsx tests/test-payment-flow.ts",
    "test:payment-dev": "npx tsx tests/test-payment.ts",
    "test:payment-prod": "npx tsx tests/test-payment-public.ts",
    "test:frontend-flow": "npx tsx tests/test-frontend-flow.ts",
    "env:switch": "npx tsx scripts/env-switch.ts",
    "deploy": "./scripts/deploy.sh",
    "deploy:dev": "./scripts/deploy.sh dev deploy",
    "deploy:test": "./scripts/deploy.sh test deploy",
    "deploy:prod": "./scripts/deploy.sh prod deploy",
    "start:dev": "./scripts/deploy.sh dev start",
    "start:test": "./scripts/deploy.sh test start",
    "start:prod": "./scripts/deploy.sh prod start",
    "stop:dev": "./scripts/deploy.sh dev stop",
    "stop:test": "./scripts/deploy.sh test stop",
    "stop:prod": "./scripts/deploy.sh prod stop",
    "status": "./scripts/deploy.sh status",
    "logs:dev": "./scripts/deploy.sh dev logs",
    "logs:test": "./scripts/deploy.sh test logs",
    "logs:prod": "./scripts/deploy.sh prod logs"
  },
  "keywords": [
    "fitness",
    "presales",
    "payment",
    "react",
    "express"
  ],
  "author": "FitActive Team",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "tsx": "^4.19.1",
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
