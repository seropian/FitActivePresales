{
  "name": "fitactive-presales-starter",
  "version": "0.0.1",
  "description": "FitActive presales application with payment processing",
  "type": "module",
  "private": true,
  "homepage": "https://presale.fitactive.open-sky.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/seropian/FitActivePresales.git"
  },
  "bugs": {
    "url": "https://github.com/seropian/FitActivePresales/issues"
  },
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Please use npm instead of yarn')\"",
    "postinstall": "npm run install:all",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "dev:server": "cd server && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:server",
    "build:frontend": "cd frontend && npm run build",
    "build:server": "cd server && npm run build",
    "install:all": "npm install && cd server && npm install && cd ../frontend && npm install",
    "clean": "npm run clean:frontend && npm run clean:server",
    "clean:frontend": "cd frontend && npm run clean",
    "clean:server": "cd server && npm run clean",
    "test:payment": "npx tsx tests/integration/test-payment-flow.ts",
    "test:payment-dev": "npx tsx tests/integration/test-payment.ts",
    "test:payment-prod": "npx tsx tests/integration/test-payment-public.ts",
    "test:frontend-flow": "npx tsx tests/e2e/test-frontend-flow.ts",
    "env:switch": "npx tsx scripts/env-switch.ts",
    "deploy": "./scripts/deploy.sh",
    "deploy:dev": "./scripts/deploy.sh dev deploy",
    "deploy:test": "./scripts/deploy.sh test deploy",
    "deploy:prod": "./scripts/deploy.sh prod deploy",
    "start:dev": "./scripts/deploy.sh dev start",
    "start:test": "./scripts/deploy.sh test start",
    "start:prod": "./scripts/deploy.sh prod start",
    "stop:dev": "./scripts/deploy.sh dev stop",
    "stop:test": "./scripts/deploy.sh test stop",
    "stop:prod": "./scripts/deploy.sh prod stop",
    "status": "./scripts/deploy.sh status",
    "logs:dev": "./scripts/deploy.sh dev logs",
    "logs:test": "./scripts/deploy.sh test logs",
    "logs:prod": "./scripts/deploy.sh prod logs"
  },
  "keywords": [
    "fitness",
    "presales",
    "payment",
    "react",
    "express"
  ],
  "author": "FitActive Team",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "tsx": "^4.19.1",
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
